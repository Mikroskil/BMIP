<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:gravity="fill"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".kalori" >
    


    <TextView
        android1:id="@+id/saran"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/imageView1"
        android1:layout_alignRight="@+id/imageView1"
        android1:layout_alignTop="@+id/imageView1"
        android1:layout_marginTop="16dp"
        android1:layout_marginRight="16dp"
        android1:layout_marginLeft="16dp"
        android1:text="TextView" />

    <TextView
        android1:id="@+id/htambah"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/saran"
        android1:layout_alignRight="@+id/saran"
        android1:layout_below="@+id/textView1"
        android1:layout_marginTop="16dp"
        android1:text="TextView" />

    <TextView
        android1:id="@+id/textView2"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/textView1"
        android1:layout_alignRight="@+id/textView1"
        android1:layout_below="@+id/htambah"
        android1:layout_marginTop="16dp"
        android1:text="TextView" />



    <TextView
        android1:id="@+id/komen"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/htambah"
        android1:layout_alignRight="@+id/htambah"
        android1:layout_alignTop="@+id/imageView2"
        android1:layout_marginTop="15dp"
        android1:text="TextView" />

    <TextView
        android1:id="@+id/textView3"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/komen"
        android1:layout_alignRight="@+id/komen"
        android1:layout_below="@+id/komen"
        android1:layout_marginTop="16dp"
        android1:text="TextView" />

    <TextView
        android1:id="@+id/textView1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_below="@+id/saran"
        android1:layout_centerHorizontal="true"
        android1:layout_marginTop="16dp"
        android1:text="TextView" />

</RelativeLayout>
